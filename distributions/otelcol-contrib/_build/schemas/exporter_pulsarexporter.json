{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "auth": {
      "properties": {
        "athenz": {
          "properties": {
            "key_id": {
              "type": "string"
            },
            "principal_header": {
              "type": "string"
            },
            "private_key": {
              "type": "string"
            },
            "provider_domain": {
              "type": "string"
            },
            "tenant_domain": {
              "type": "string"
            },
            "tenant_service": {
              "type": "string"
            },
            "zts_url": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "oauth2": {
          "properties": {
            "audience": {
              "type": "string"
            },
            "client_id": {
              "type": "string"
            },
            "issuer_url": {
              "type": "string"
            },
            "private_key": {
              "type": "string"
            },
            "scope": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "tls": {
          "properties": {
            "cert_file": {
              "type": "string"
            },
            "key_file": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "token": {
          "properties": {
            "token": {
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "connection_timeout": {
      "description": "Duration string (e.g., '1s', '5m', '1h')",
      "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
      "type": "string"
    },
    "enabled": {
      "description": "Enabled indicates whether to not retry sending batches in case of export failure.",
      "type": "boolean"
    },
    "encoding": {
      "description": "Encoding of messages (default \"otlp_proto\")",
      "type": "string"
    },
    "endpoint": {
      "description": "Endpoint of pulsar broker (default \"pulsar://localhost:6650\")",
      "type": "string"
    },
    "initial_interval": {
      "description": "InitialInterval the time to wait after the first failure before retrying.",
      "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
      "type": "string"
    },
    "max_connections_per_broker": {
      "type": "integer"
    },
    "max_elapsed_time": {
      "description": "MaxElapsedTime is the maximum amount of time (including retries) spent trying to send a request/batch. Once this value is reached, the data is discarded. If set to 0, the retries are never stopped.",
      "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
      "type": "string"
    },
    "max_interval": {
      "description": "MaxInterval is the upper bound on backoff interval. Once this value is reached the delay between consecutive retries will always be `MaxInterval`.",
      "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
      "type": "string"
    },
    "multiplier": {
      "description": "Multiplier is the value multiplied by the backoff interval bounds",
      "type": "number"
    },
    "operation_timeout": {
      "description": "Duration string (e.g., '1s', '5m', '1h')",
      "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
      "type": "string"
    },
    "producer": {
      "description": "Producer configuration of the Pulsar producer",
      "properties": {
        "batch_builder_type": {
          "type": "string"
        },
        "batching_max_messages": {
          "type": "integer"
        },
        "batching_max_publish_delay": {
          "description": "Duration string (e.g., '1s', '5m', '1h')",
          "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
          "type": "string"
        },
        "batching_max_size": {
          "type": "integer"
        },
        "compression_level": {
          "type": "string"
        },
        "compression_type": {
          "type": "string"
        },
        "disable_batching": {
          "type": "boolean"
        },
        "disable_block_if_queue_full": {
          "type": "boolean"
        },
        "hashing_scheme": {
          "type": "string"
        },
        "max_pending_messages": {
          "type": "integer"
        },
        "max_reconnect_broker": {
          "type": "integer"
        },
        "partitions_auto_discovery_interval": {
          "description": "Duration string (e.g., '1s', '5m', '1h')",
          "pattern": "^[0-9]+(ns|us|µs|ms|s|m|h)$",
          "type": "string"
        }
      },
      "type": "object"
    },
    "randomization_factor": {
      "description": "RandomizationFactor is a random factor used to calculate next backoffs Randomized interval = RetryInterval * (1 ± RandomizationFactor)",
      "type": "number"
    },
    "sending_queue": {
      "type": "object"
    },
    "timeoutsettings": {
      "type": "object"
    },
    "tls_allow_insecure_connection": {
      "description": "Configure whether the Pulsar client accept untrusted TLS certificate from broker (default: false)",
      "type": "boolean"
    },
    "tls_trust_certs_file_path": {
      "description": "Set the path to the trusted TLS certificate file",
      "type": "string"
    },
    "topic": {
      "description": "The name of the pulsar topic to export to (default otlp_spans for traces, otlp_metrics for metrics)",
      "type": "string"
    }
  },
  "type": "object"
}